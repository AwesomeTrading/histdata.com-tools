{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: python -m histdatacom",
            "type": "python",
            "request": "launch",
            "module": "histdatacom",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "args": [
                "-DXI",
                "-p","usdcad",
                "-P","ascii",
                "-t","1-minute-bar-quotes","tick-data-quotes",//
                "-s","2022-04"
            ],
            "justMyCode": true
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        }
    ]
}