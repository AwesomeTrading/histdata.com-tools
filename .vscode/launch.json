{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: python -m histdatacom",
            "type": "python",
            "request": "launch",
            "module": "histdatacom",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "args": [
                "-DXCI",
                "-p","usdmxn",
                "-P","ascii",
                "-t","tick-data-quotes",
                "-s","2020-07",
                "-e","2022-04"
            ],
            "justMyCode": true
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        }
    ]
}