{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: python -m histdatacom",
            "type": "python",
            "request": "launch",
            "module": "histdatacom",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "args": [
                "-I",
                "-p","usdjpy",
                "-f","ascii",
                "-t","tick-data-quotes",//"1-minute-bar-quotes",//
                "-s","now",
                "-e","now"
            ],
            "justMyCode": true
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        }
    ]
}