{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: python -m histdatacom",
            "type": "python",
            "request": "launch",
            "module": "histdatacom",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "args": [
                "-I"
                "-p","eurcad",
                "-f","ascii",
                "-t","1-minute-bar-quotes",//"tick-data-quotes",//
                "-s","2022-01",
                "-e","now",
                "-c","100"
            ],
            "justMyCode": true
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "prospector",
            "type": "python",
            "request": "launch",
            "module": "prospector",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "args": [
                ">","prospector_results.json"
            ],
            "justMyCode": true
        }
    ]
}